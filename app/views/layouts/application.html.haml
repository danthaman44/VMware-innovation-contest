%html
  %head
    %title Innovation Contest
    = stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true
    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700|Merriweather:400,300,700|Open+Sans+Condensed:700,300,300italic"
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags


  %body
    = render 'layouts/header'

    %section.content
      .container
        = render 'layouts/breadcrumbs'
        = yield

    = render 'layouts/footer'